<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset>
		<field name="id" type="text" default="0" label="JGLOBAL_FIELD_ID_LABEL"
			readonly="true" class="readonly"
			description="JGLOBAL_FIELD_ID_DESC"/>

		<field name="name" type="text" class="inputbox"
			size="40" label="JGLOBAL_TITLE"
			description="COM_WEBLINKS_FIELD_TITLE_DESC" required="true" />

		<field
			name="db_id"
			type="list"
			label="JSTATUS"
			description="COM_WEBLINKS_FIELD_STATE_DESC"
			class="inputbox"
			size="1"
			default="1">
			<option
				value="1">JPUBLISHED</option>
			<option
				value="0">JUNPUBLISHED</option>
			<option
				value="2">JARCHIVED</option>
			<option
				value="-3">COM_WEBLINKS_FIELD_VALUE_REPORTED</option>
			<option
				value="-2">JTRASHED</option>
		</field>
                <field
			name="shop_system_id"
			type="list"
			label="JSTATUS"
			description="COM_WEBLINKS_FIELD_STATE_DESC"
			class="inputbox"
			size="1"
			default="1">
			<option
				value="1">JPUBLISHED</option>
			<option
				value="0">JUNPUBLISHED</option>
			<option
				value="2">JARCHIVED</option>
			<option
				value="-3">COM_WEBLINKS_FIELD_VALUE_REPORTED</option>
			<option
				value="-2">JTRASHED</option>
		</field>

		<field id="status" name="status" type="radio"
			class="inputbox" label="WEBLINKS_APPROVED_LABEL"
			description="WEBLINKS_APPROVED_DESC"
		>
			<option value="0">JNO</option>
			<option value="1">JYES</option>
		</field>
	    
            </fieldset>
</form>
